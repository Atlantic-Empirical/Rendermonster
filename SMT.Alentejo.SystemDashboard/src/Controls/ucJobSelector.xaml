<UserControl x:Class="ucJobSelector"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Width="426" Height="300">
    <Grid Background="White">
        <ComboBox Height="23" Margin="12,11,0,0" Name="cbDateRange" VerticalAlignment="Top" HorizontalAlignment="Left" Width="120">
            <ComboBoxItem>All</ComboBoxItem>
            <ComboBoxItem>Last 7 Days</ComboBoxItem>
            <ComboBoxItem>Last 30 Days</ComboBoxItem>
        </ComboBox>
        <Button Height="23" HorizontalAlignment="Right" Margin="0,0,93,8" Name="btnOk" VerticalAlignment="Bottom" Width="75">OK</Button>
        <TextBox Height="23" Margin="138,11,0,0" Name="txtKeyWord" VerticalAlignment="Top" HorizontalAlignment="Left" Width="230" />
        <Button Height="23" Margin="374,11,0,0" Name="btnKeywordSearch" VerticalAlignment="Top" HorizontalAlignment="Left" Width="14">.</Button>
        <ListBox Margin="12,40,12,37" Name="lbJobs" />
    </Grid>
</UserControl>
